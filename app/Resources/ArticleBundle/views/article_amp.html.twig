<section class="article-page">
	<div class="container content">
		{% block breadchumbs %}
			{{
				sonata_block_render(
					{
						'type': 'app.block.service.breadcrumb'
					}
				)
			}}
		{% endblock %}

		<div class="article-wrapper main-content floating-container">
			<div class="article-content">
				<div class="title">
					<h1>{{ item.item_title(article) }}{{ item.item_label(article) }}</h1>

					{% if (article['isAdvHidden'] is not defined) or (not article['isAdvHidden']) %}
						{{
						sonata_block_render(
							{ 'type': 'page.block.site_variable' },
							{
								'placement': 'banner-content-amp'
							}
						)
						}}
						{# <amp-auto-ads type="adsense" data-ad-client="ca-pub-4655607803669314"></amp-auto-ads> #}
					{% endif %}

					<div class="time">
						<span>{{ article.publishAt|date_time_humanize() }}</span>
					</div>
				</div><!-- /.title -->

				{% if article.getHeader() is not empty %}
					<div class="sub-title">
						<p>{{ article.getHeader()|striptags }}</p>
					</div>
				{% endif %}

				{% include 'AppBundle:Block:social_share_amp.html.twig' %}

				{% if article.showImage %}
					{% set mainImage = article.getImage() %}
					{% if mainImage %}
						{% block article_image %}
							{% set mainImagePath = mainImage.getPath() %}
							<figure>
								<amp-img src="{{ mainImagePath|get_actual_image('image_610x343', ['main_new', 'main']) }}" width="610" height="343" layout="responsive"></amp-img> <!-- amp-img -->
							</figure>
						{% endblock %}
					{% endif %}
				{% endif %}

				<span>
					{{ article.translate.description|shortcode(true, 'amp')|amp_text|raw }}

					{% if article.tags is defined and article.tags is not empty %}
						<div class="tag">
							{% for tag in article.tags %}
								<a href={{ path('articles_tag', {'slug' : tag.slug}) }}>{{ tag.name }}</a>{{ loop.last ? '' : ', ' }}
							{% endfor %}
						</div>
					{% endif %}

					{% include 'AppBundle:Block:social_share_amp.html.twig' %}
				</span>

				{% if article.related is not empty %}
					<div class="read-also">
						<h3 class="title">{{ 'front.read_also'|trans([], 'ArticleBundle') }}</h3>
						{% for relatedArticle in article.related %}
							{% if not loop.last or loop.last and loop.index % 2 == 0 %}
								{{ item.item_middle(relatedArticle, loop) }}
							{% endif %}
						{% endfor %}
					</div><!-- /.read-also -->
				{% endif %}
			</div><!-- /.article-content -->
		</div><!-- /.article-wrapper -->
	</div><!-- /.container -->
</section><!-- /.article-page -->
